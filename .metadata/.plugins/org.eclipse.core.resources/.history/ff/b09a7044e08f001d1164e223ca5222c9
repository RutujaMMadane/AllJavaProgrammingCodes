package variablesAndDataTypes;

public enum DaysDemo {
	
	MONDAY(0),
	TUESDAY(1),
	WEDNESDAY(2),
	THRUSDAY(4),
	FRIDAY(5),
	SATURDAY(6);
	
	int daysindex;
	DaysDemo(int daysindex)
	{
		this.daysindex=daysindex;
	}
	public int getDaysindex() {
		return daysindex;
	}
	public void setDaysindex(int daysindex) {
		this.daysindex = daysindex;
	}
	

}
