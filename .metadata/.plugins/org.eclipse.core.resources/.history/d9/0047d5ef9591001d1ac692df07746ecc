package Assessment;
import java.util.*;
public class Pattern {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int col;
		int row;
		for(int row=1;row<=5;row++)
		{
			for(int col=row-1;col<=row;col++)
			
			{
				System.out.print(" ");
			}
			for(int col=1;col<=row;col++)
			{
				System.out.print(row+" ");
			}
			System.out.println();
		}	
	}
	

}
